<link rel="stylesheet" href="/css/agent.css">

<table class="uk-table uk-table-striped">
    <tr>
        <% if(agent != null) { %>
        <% if(agent.a_image1 == null) { %>
            <td></td>
        <% } else { %>
            <td><img src="/uploads/<%= agent.a_image1 %>" /></td>
        <% } %>
        <% if(agent.a_image2 == null) { %>
            <td></td>
        <% } else { %>
            <td><img src="/uploads/<%= agent.a_image2 %>" /></td>
        <% } %>
        <% if(agent.a_image3 == null) { %>
            <td></td>
        <% } else { %>
            <td><img src="/uploads/<%= agent.a_image3 %>" /></td>
        <% } %>
        <p><%= agent.a_introduction %></p>
        <% } %>
    </tr>
</table>