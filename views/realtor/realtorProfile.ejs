<link rel="stylesheet" href="/css/agent.css">

<div class="row">
    <div class="left">
      <div class="photo-left">
        <% if(agentPrivate == null) { %>
            <img class = "photo" src="/images/none-image-status.jpg">
        <% } else { %>
            <img class = "photo" src="/uploads/<%= agentPrivate.a_profile_image %>" />
            <% } %>
      </div>
      
      <% if(agent == null) { %>
        <div>정보없음</div>
      <% } else { %>
        <h4 class="name"><%= agent.RDEALER_NM %></h4>
        <% } %>

    <% if(agent == null) { %>
        <div>정보없음</div>
    <% } else { %>
        <p class="info"><%= agent.CMP_NM %></p>
    <% } %>

    <% if(agent == null) { %>
        <div>정보없음</div>
    <% } else { %>
        <p class="info"><%= agent.ADDRESS %></p>
    <% } %>

    <span>평균 별점</span>
    <span class="uk-badge"><%= rating %></span>

</div>